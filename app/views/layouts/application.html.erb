
<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<%= csrf_meta_tag %>
		<%= javascript_include_tag :application %>
		<%= stylesheet_link_tag :application %>
		<title>Go Go Go</title>
	</head>
	<body>
		<header>
			<h1>GO GO GO</h1>
			<% if user_signed_in? %>
				<%= link_to "Logout", destroy_user_session_path, method: :delete %>
			<% end %>
		</header>
		<article>
			<%= yield %>
		</article>
	</body>
</html>
